# MAIN STREAMLIT APPLICATION
# =============================================================================

def main():
    """Main Streamlit application"""
    
    # Setup page configuration and styling
    setup_page_config()
    
    # Header with sci-fi styling
    st.markdown("""
    <div style='text-align: center; padding: 20px;'>
        <h1 style='color: #00D4AA; font-size: 3em; text-shadow: 0 0 20px #00D4AA;'>
            üöÄ AI STOCK ANALYZER
        </h1>
        <p style='color: #E0E1DD; font-size: 1.2em;'>
            Multi-Agent AI System for Comprehensive Stock Analysis
        </p>
    </div>
    """, unsafe_allow_html=True)
    
    # Sidebar configuration
    with st.sidebar:
        st.markdown("### üéõÔ∏è Analysis Controls")
        
        # Stock ticker input
        ticker = st.text_input(
            "Stock Ticker",
            value="AAPL",
            help="Enter stock symbol (e.g., AAPL, GOOGL, TSLA)"
        ).upper()
        
        # Time period selection
        period = st.selectbox(
            "Analysis Period",
            ["1y", "6mo", "3mo", "1mo"],
            index=0,
            help="Select time period for historical data"
        )
        
        # Analysis button
        analyze_button = st.button(
            "üöÄ Launch Analysis",
            type="primary",
            use_container_width=True
        )
        
        st.markdown("---")
        st.markdown("""
        ### ü§ñ AI Agents
        - **Technical Analyst**: Chart patterns & indicators
        - **Fundamental Analyst**: Financial health & valuation  
        - **Risk Manager**: Risk assessment & position sizing
        - **Portfolio Manager**: Final investment decision
        """)
        
        st.markdown("---")
        st.markdown("### ‚ö†Ô∏è Disclaimer")
        st.caption("This is for educational purposes only. Not financial advice.")
    
    # Main content area
    if analyze_button:
        if not OPENAI_API_KEY or OPENAI_API_KEY == "YOUR_OPENAI_API_KEY_HERE":
            st.error("‚ö†Ô∏è Please add your OpenAI API key in the code configuration section!")
            st.code("OPENAI_API_KEY = 'your-actual-api-key-here'")
            return
        
        # Fetch stock data
        with st.spinner(f'üìä Fetching data for {ticker}...'):
            stock_data = get_stock_data(ticker, period)
        
        if stock_data is None:
            st.error(f"‚ùå Could not fetch data for {ticker}. Please check the ticker symbol.")
            return
        
        # Display company information
        st.markdown(f"""
        <div class='main-container'>
            <h2>üìà {stock_data['company_info']['name']} ({ticker})</h2>
            <p><strong>Sector:</strong> {stock_data['company_info']['sector']}</p>
            <p><strong>Industry:</strong> {stock_data['company_info']['industry']}</p>
        </div>
        """, unsafe_allow_html=True)
        
        # Display key metrics
        st.markdown("### üìä Key Metrics")
        display_key_metrics(stock_data)
        
        # Create tabs for different views
        tab1, tab2, tab3, tab4 = st.tabs(["üìà Charts", "üîç Technical", "üí∞ Fundamentals", "ü§ñ AI Analysis"])
        
        with tab1:
            st.markdown("### Price Chart")
            price_chart = create_price_chart(stock_data['historical_data'], ticker)
            st.plotly_chart(price_chart, use_container_width=True)
            
            col1, col2 = st.columns(2)
            with col1:
                st.markdown("### RSI Indicator")
                rsi_chart = create_rsi_chart(stock_data['historical_data'])
                st.plotly_chart(rsi_chart, use_container_width=True)
            
            with col2:
                st.markdown("### Volume Trend")
                volume_chart = go.Figure()
                volume_chart.add_trace(go.Scatter(
                    x=stock_data['historical_data'].index,
                    y=stock_data['historical_data']['Volume'],
                    fill='tonexty',
                    name='Volume',
                    line=dict(color='#00D4AA')
                ))
                volume_chart.update_layout(
                    title='Trading Volume',
                    template='plotly_dark',
                    height=300
                )
                st.plotly_chart(volume_chart, use_container_width=True)
        
        with tab2:
            st.markdown("### üîç Technical Analysis Summary")
            
            tech_data = stock_data['technical']
            
            col1, col2 = st.columns(2)
            
            with col1:
                st.markdown("#### üìä Price vs Moving Averages")
                sma_data = {
                    'SMA 20': {'price': tech_data['sma_20'], 'status': 'üü¢ Above' if tech_data['above_sma_20'] else 'üî¥ Below'},
                    'SMA 50': {'price': tech_data['sma_50'], 'status': 'üü¢ Above' if tech_data['above_sma_50'] else 'üî¥ Below'},
                    'SMA 200': {'price': tech_data['sma_200'], 'status': 'üü¢ Above' if tech_data['above_sma_200'] else 'üî¥ Below'}
                }
                
                for sma, info in sma_data.items():
                    st.write(f"**{sma}**: ${info['price']:.2f} - {info['status']}")
            
            with col2:
                st.markdown("#### ‚ö° Momentum Indicators")
                
                rsi_status = "üü¢ Neutral" if 30 <= tech_data['rsi'] <= 70 else "üî¥ Overbought" if tech_data['rsi'] > 70 else "üü° Oversold"
                st.write(f"**RSI**: {tech_data['rsi']:.1f} - {rsi_status}")
                
                macd_status = "üü¢ Bullish" if tech_data['macd'] > tech_data['macd_signal'] else "üî¥ Bearish"
                st.write(f"**MACD**: {macd_status}")
                
                st.write(f"**Volume**: {tech_data['volume_trend']}")
        
        with tab3:
            st.markdown("### üí∞ Fundamental Analysis Summary")
            
            fund_data = stock_data['fundamental']
            
            col1, col2 = st.columns(2)
            
            with col1:
                st.markdown("#### üìä Valuation Metrics")
                if fund_data['pe_ratio']:
                    pe_status = "üü¢ Reasonable" if fund_data['pe_ratio'] < 25 else "üî¥ High"
                    st.write(f"**P/E Ratio**: {fund_data['pe_ratio']:.1f} - {pe_status}")
                
                if fund_data['pb_ratio']:
